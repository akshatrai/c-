#include <iostream>


using namespace std;

struct student
{
  int roll_no;
  char name[20];
  int marks;
};

int main()
{

	struct student stud[3];
  	int i;

	for(i=0; i<3; i++){                   		
		cout << "Student " << i + 1 << endl;
		cout << "Enter roll no" << endl;
		cin >> stud[i].roll_no;
		cout << "Enter name" << endl;
		cin >> stud[i].name;
		cout << "marks" << endl;
		cin >> stud[i].marks;
	}

	for(i=0; i<3; i++){							
		cout << "Student " << i + 1 << endl;
		cout << "Roll no : " << stud[i].roll_no << endl;
		cout << "Name : " << stud[i].name << endl;
		cout << "marks : " << stud[i].marks << endl;
	}
	return 0;
}
